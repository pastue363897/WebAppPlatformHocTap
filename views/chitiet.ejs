    <meta name="description" content="">
    <meta name="author" content="">

    <title>Chi tiết khóa học</title>

    <!-- Bootstrap core CSS -->
    <link href="stylesheets/assets/bootstrap.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="stylesheets/style_chitiet.css" rel="stylesheet">
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="stylesheets/assets/bootstrap.min.css">
    <!-- Font awsome CSS -->
    <link rel="stylesheet" href="stylesheets/assets/font-awesome.min.css">
    <link rel="stylesheet" href="stylesheets/assets/flaticon.css">
    <link rel="stylesheet" href="stylesheets/assets/magnific-popup.css">
    <!-- owl carousel -->
    <link rel="stylesheet" href="stylesheets/assets/owl.carousel.css">
    <link rel="stylesheet" href="stylesheets/assets/owl.theme.css">

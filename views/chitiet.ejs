    <link rel="stylesheet" href="stylesheets/responsive.css">
    <link rel="stylesheet" href="stylesheets/demo.css">

</head>
<body>
    <header class="header_inner courses_page">
        <!-- Preloader -->
        <div class="header_top">
            <div class="container">
                <div class="row">
                    <div class="col-12 col-sm-12 col-lg-12">
                        <div class="info_wrapper">
                            <div class="contact_info">
                                <ul class="list-unstyled">
                                    <li><i class="flaticon-phone-receiver"></i>+84 974979691</li>
                                    <li><i class="flaticon-mail-black-envelope-symbol"></i>duykin1235@gmail.com</li>
                                </ul>
                            </div>
                            <div class="login_info">
                                <ul class="d-flex">
                                    <% if(!uname) { %>
                                    <li class="nav-item"><a href="#" class="nav-link join_now js-modal-show">Đăng Ký /
                                            Đăng Nhập</a>
                                    </li>
                                    <% } else { %>
                                    <li class="nav-item"><a href="#" class="nav-link sign-in"><i
                                                class="flaticon-user-male-black-shape-with-plus-sign"></i>
                                            Chào mừng, <%= uname %> | Số tiền đang có: <%= balance %></a></li>
                                            <% if(type == 1) %><li class="nav-item"><a href="/naptien" class="nav-link sign-in">Nạp tiền</a></li><% } %>
                                    <li class="nav-item"><a href="/logout" class="nav-link join_now"><i
                                                class="flaticon-padlock"></i>Đăng xuất</a></li>
                                    <% } %>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="edu_nav">
            <div class="container">
                <nav class="navbar navbar-expand-md navbar-light bg-faded">
                    <a class="navbar-brand" href="/"><img style="height: 50px" src="images/logo.png" alt="logo"></a>
                    <div class="collapse navbar-collapse main-menu" id="navbarSupportedContent">
                        <ul class="navbar-nav nav lavalamp ml-auto menu">
                            <li class="nav-item"><a href="/" class="nav-link"
                                    style="z-index: 5; font-family:'Roboto', sans-serif; font-weight: 900; white-space: nowrap;text-transform:left;">Trang
                                    Chủ</a>
                            </li>
                            <li class="nav-item"><a href="/course" class="nav-link"
                                    style="z-index: 5; font-family:'Roboto', sans-serif; font-weight: 900; white-space: nowrap;text-transform:left;">Các
                                    Khoá Học</a>
                            </li>
                            <li class="nav-item"><a href="/course-owned" class="nav-link"
                                    style="z-index: 5; font-family:'Roboto', sans-serif; font-weight: 900; white-space: nowrap;text-transform:left;">Khoá
                                    Học Đã Mua</a>
                            </li>
                            <li class="nav-item"><a href="/about" class="nav-link"
                                    style="z-index: 5; font-family:'Roboto', sans-serif; font-weight: 900; white-space: nowrap;text-transform:left;">Về
                                    Chúng Tôi</a></li>
                        </ul>
                    </div>
                </nav><!-- END NAVBAR -->
            </div>
        </div>
    </header> <!--  End header section-->
    <!-- Page Content -->
    <div class="container">

        <div class="row">

            <div class="col-xl-4 col-lg-4 col-md-4 col-sm-12 col-12">
                <h2 class="my-4">Chi tiết khóa học</h2>
            </div>
            <!-- /.col-lg-3 -->

            <div class="col-xl-8 col-lg-8 col-md-8 col-sm-12 col-12">

                <div class="card mt-4">

                    <!-- ------------- phần hiển thị các bài học trong một khóa học --------------------------------- -->
                    <div id="giao_trinh" class="curriculum-container lazy_load" loaded="true">
                        <div class="title"><%= items[0].TenKH %></div>
                        <div class="curriculum-overview">
                            <div class="curriculum-type">
                                Chủ đề: <span><%= items[0].TenChuDe %> | </span>
                                <span>Số bài: <%= items.length %></span>
                            </div>
                            <div class="curriculum-lessons">
                                <div class="curriculum-lessons-number">

                                </div>
                            </div>
                        </div>
                        <div class="curriculum-lesson-container">
                            <div class="chapter-lessons">
                                <% for(var i = 0; i < items.length; i++) { %>
                                <div class="chappter-lesson">
                                    <div class="lesson-name-container">
                                        <a href="/video?idBH=<%= items[i].IdBaiHoc%>">
                                            <img src="http://dvkhfbm6djrbs.cloudfront.net/5be57a4be2e9ab000ba3a5b6/5d43f62ce1af2a00128ed897/ezgif.com-gif-maker.png"
                                                width="24" height="24"> Bài <%= i+1 %></a>
                                        <div class="lesson-name"><%= items[i].TenBH %></div>
                                    </div>
                                    <div class="separate-line">
                                    </div>
                                </div>
                                <% } %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- /.container -->

    <!-- Footer -->
    <footer id="footer" class="footer-1">
